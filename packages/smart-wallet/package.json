    "@endo/init": "^1.1.2",
    "ava": "^5.3.0",
    "import-meta-resolve": "^2.2.1"
  },
  "dependencies": {
    "@agoric/assert": "^0.6.0",
    "@agoric/casting": "^0.4.2",
    "@agoric/ertp": "^0.16.2",
    "@agoric/internal": "^0.3.2",
    "@agoric/notifier": "^0.6.2",
    "@agoric/store": "^0.9.2",
    "@agoric/vat-data": "^0.5.2",
    "@agoric/vats": "^0.15.1",
    "@agoric/vow": "^0.1.0",
    "@agoric/zoe": "^0.26.2",
    "@endo/eventual-send": "^1.2.2",
    "@endo/far": "^1.1.2",
    "@endo/marshal": "^1.5.0",
    "@endo/nat": "^5.0.7",
    "@endo/promise-kit": "^1.1.2"
