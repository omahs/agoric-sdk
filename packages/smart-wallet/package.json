  "files": [
    "src/"
  ],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric/issues"
  },
  "homepage": "https://github.com/Agoric/agoric#readme",
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "2m"
  },
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 94.36
